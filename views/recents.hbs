{{!--{{title}}--}}

<div class="container">
{{#each records}}
    <div class="col-sm-6 col-md-4 col-lg-3 mt-4">

            <div class="card">
                <video class="card-img-top" src="{{location}}" controls></video>
                <div class="card-block">
                    <h6 > Data :  {{duration}} </h6>
                    <h6 >Marimea : {{size}} KB </h6>
                    <button  class="btn btn-primary disable" >Sterge</button>
                </div>
            </div>
    </div>
{{/each}}
</div>
